<Window x:Class="howdoyou.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:howdoyou"
        mc:Ignorable="d"
		xmlns:gif="http://wpfanimatedgif.codeplex.com"
        Title="HowDoYou" Height="450" Width="666.601"
		WindowStyle="None"
		AllowsTransparency="True"
		Background="Transparent"
		ShowInTaskbar="False"
		>
	<Grid>
		<Image x:Name="HowDoYouSticker" gif:ImageBehavior.RepeatBehavior="Forever" gif:ImageBehavior.AnimatedSource="Images/하우두유 투명화.gif" HorizontalAlignment="Left" Height="450" VerticalAlignment="Top" Width="667" Source="Images/하우두유 투명화.gif" RenderTransformOrigin="0.5,0.5" MouseDown="HowDoYouSticker_MouseDown">
			<Image.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform AngleX="-0.208"/>
					<RotateTransform/>
					<TranslateTransform X="-0.886"/>
				</TransformGroup>
			</Image.RenderTransform>
		</Image>
	</Grid>
</Window>
